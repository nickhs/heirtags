{
  "name": "heirtags",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "fs-promise": "^2.0.2",
    "jest": "^19.0.2",
    "webpack": "^2.4.1"
  },
  "scripts": {
    "test": "NODE_ENV=test jest --coverage"
  },
  "jest": {
    "testRegex": ".*test.*.js",
    "roots": [
      "src",
      "tests"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/internals/test-bundler.js"
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "stage-0"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "dependencies": {
    "minimatch": "^3.0.3"
  }
}
